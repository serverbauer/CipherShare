generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model SharedPassword {
  id             String   @id @default(uuid())
  password       String
  expirationDate DateTime
  usageLimit     Int
  usageCount     Int      @default(0)
  accessPassword String
  createdAt      DateTime @default(now())
}

